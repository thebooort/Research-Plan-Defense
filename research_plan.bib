	keywords = {Network analysis, Community structures, Data mining, Graph analysis, Graph mining, Graph theory, Network inference, Relational data, Social interactions, Social network, Social network analysis, Statistical analysis, Statistical ensemble},
	pages = {111--120},
	file = {Springer Full Text PDF:/home/booort/Zotero/storage/9UE7HLLJ/Casiraghi et al. - 2017 - From Relational Data to Graphs Inferring Signific.pdf:application/pdf}
}

