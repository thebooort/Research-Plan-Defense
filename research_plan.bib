	keywords = {Food, Algorithms, Diabetes Mellitus, Energy Intake, food recognition, Humans, Mobile Applications, mobile food recognition, Nutrition Assessment, nutrition estimation, Photography, Precision Medicine, Reproducibility of Results, Smartphone, Support Vector Machine, visual food recognition, Diabetic, Diet},
